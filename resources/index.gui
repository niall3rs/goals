  </use>
  
  <use id="new-goal" class="home-button" href="#square-button" y="5" fill="blue">
    <set href="#text" attributeName="text-buffer" to="New goal" />
  </use>
  
  <use id="goals" class="home-button" href="#square-button" y="$" fill="yellow">
